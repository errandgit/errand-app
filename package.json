{
  "name": "errand-app",
  "version": "1.0.0",
  "description": "A service marketplace application similar to Thumbtack",
  "main": "backend/app.js",
  "scripts": {
    "start": "node backend/app.js",
    "server": "nodemon backend/app.js",
    "client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "marketplace",
    "services",
    "thumbtack"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^3.1.0"
  }
}
